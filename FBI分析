drop table [通用].[dbo].[FBI]
	SELECT [通用].[dbo].[客户财务信息表].集团
	,[通用].[dbo].[客户财务信息表].集团全称
	,[通用].[dbo].[客户财务信息表].归属行业
	,[通用].[dbo].[客户财务信息表].客户一级别
	,[通用].[dbo].[客户财务信息表].客户二级别
	,[通用].[dbo].[客户财务信息表].客户所属分公司
	,[通用].[dbo].[客户财务信息表].客户渠道
	,[账务明细].[dbo].[账务明细全].* into  [通用].[dbo].[FBI] FROM 
	[账务明细].[dbo].[账务明细全] left join [通用].[dbo].[客户财务信息表]
	on [账务明细].[dbo].[账务明细全].客户编号金蝶 = [通用].[dbo].[客户财务信息表].会员号
