SELECT distinct 客户编号,客户名称,来款单位,len(来款单位) as A
  FROM [通用].[dbo].[录款信息]
  where 集团名称 like '南方电网%' and 收款单状态 = '已通过' and 客户名称 not like CONCAT('%',来款单位,'%') and left(来款单位,1) <>'收' and len(来款单位)>4
  order by len(来款单位)
  
  
SELECT A2.* FROM
(SELECT distinct 来款单位
  FROM [通用].[dbo].[录款信息]
  where 集团名称 like '南方电网%'and 收款单状态 = '已通过' And left(来款单位,1) <>'收' and len(来款单位)>4) AS A1
INNER JOIN
(SELECT 收款编号,来款单位,集团名称
  FROM [通用].[dbo].[录款信息]
  where 集团名称 not like '南方电网%'and 收款单状态 = '已通过') as A2
ON A1.来款单位 = A2.来款单位

select * FROM [通用].[dbo].[录款信息]
where 集团名称 like '国家税务总局%' and 来款单位='上海晨光科力普办公用品有限公司深圳分公司'
